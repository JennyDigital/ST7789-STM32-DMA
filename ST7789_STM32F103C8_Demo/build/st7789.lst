 215:Core/Src/st7789.c **** /**
 216:Core/Src/st7789.c ****  * @brief Fill the DisplayWindow with single color
 217:Core/Src/st7789.c ****  * @param color -> color to Fill with
 218:Core/Src/st7789.c ****  * @return none
 219:Core/Src/st7789.c ****  */
 220:Core/Src/st7789.c **** void ST7789_Fill_Color(uint16_t color) {
 221:Core/Src/st7789.c ****     uint16_t i;
 222:Core/Src/st7789.c ****     ST7789_SetAddressWindow(0, 0, ST7789_WIDTH - 1, ST7789_HEIGHT - 1);
 223:Core/Src/st7789.c ****     ST7789_Select();
 224:Core/Src/st7789.c **** 
 225:Core/Src/st7789.c ****     #ifdef USE_DMA
 226:Core/Src/st7789.c **** 
 227:Core/Src/st7789.c ****     for (i = 0; i < (ST7789_HEIGHT * ST7789_WIDTH) / (sizeof(disp_buf) / 2); i++) {
 228:Core/Src/st7789.c ****         memset_16((uint16_t*)disp_buf, color, sizeof(disp_buf) / 2); 
 229:Core/Src/st7789.c ****         SendBuffer((uint16_t*)disp_buf, sizeof(disp_buf) / 2);     
 230:Core/Src/st7789.c ****     }
 231:Core/Src/st7789.c ****     #else
 232:Core/Src/st7789.c ****     uint16_t j;
 233:Core/Src/st7789.c ****     for (i = 0; i < ST7789_HEIGHT; i++) {
 234:Core/Src/st7789.c ****         for (j = 0; j < ST7789_WIDTH; j++) {
 235:Core/Src/st7789.c ****             uint8_t data[] = {color >> 8, color & 0xFF}; 
 236:Core/Src/st7789.c ****             ST7789_WriteData(data, sizeof(data));
 237:Core/Src/st7789.c ****         }
 238:Core/Src/st7789.c ****     }
 239:Core/Src/st7789.c ****     #endif
 240:Core/Src/st7789.c **** 
 241:Core/Src/st7789.c ****     ST7789_UnSelect();
 242:Core/Src/st7789.c **** }
