  39:Core/Src/st7789.c **** /**
  40:Core/Src/st7789.c ****  * @brief Write data to ST7789 controller
  41:Core/Src/st7789.c ****  * @param buff -> pointer of data buffer
  42:Core/Src/st7789.c ****  * @param buff_size -> size of the data buffer
  43:Core/Src/st7789.c ****  * @return none
  44:Core/Src/st7789.c ****  */
  45:Core/Src/st7789.c **** static void ST7789_WriteData(uint8_t *buff, size_t buff_size)
  46:Core/Src/st7789.c **** {
  47:Core/Src/st7789.c **** 	ST7789_Select();
  48:Core/Src/st7789.c **** 	ST7789_DC_Set();
  49:Core/Src/st7789.c **** 
  50:Core/Src/st7789.c **** 	// split data in small chunks because HAL can't send more than 64K at once
  51:Core/Src/st7789.c **** 
  52:Core/Src/st7789.c **** 	while (buff_size > 0) {
  53:Core/Src/st7789.c **** 		uint16_t chunk_size = buff_size > 65535 ? 65535 : buff_size;
  54:Core/Src/st7789.c **** 		#ifdef USE_DMA
  55:Core/Src/st7789.c **** 			if (DMA_MIN_SIZE <= buff_size)
  56:Core/Src/st7789.c **** 			{
  57:Core/Src/st7789.c **** 				HAL_SPI_Transmit_DMA(&ST7789_SPI_PORT, buff, chunk_size);
  58:Core/Src/st7789.c **** 				while (ST7789_SPI_PORT.hdmatx->State != HAL_DMA_STATE_READY)
  59:Core/Src/st7789.c **** 				{}
  60:Core/Src/st7789.c **** 			}
  61:Core/Src/st7789.c **** 			else
  62:Core/Src/st7789.c **** 				HAL_SPI_Transmit(&ST7789_SPI_PORT, buff, chunk_size, HAL_MAX_DELAY);
  63:Core/Src/st7789.c **** 		#else
  64:Core/Src/st7789.c **** 			HAL_SPI_Transmit(&ST7789_SPI_PORT, buff, chunk_size, HAL_MAX_DELAY);
  65:Core/Src/st7789.c **** 		#endif
  66:Core/Src/st7789.c **** 		buff += chunk_size;
  67:Core/Src/st7789.c **** 		buff_size -= chunk_size;
  68:Core/Src/st7789.c **** 	}
  69:Core/Src/st7789.c **** 
  70:Core/Src/st7789.c **** 	ST7789_UnSelect();
  71:Core/Src/st7789.c **** }
