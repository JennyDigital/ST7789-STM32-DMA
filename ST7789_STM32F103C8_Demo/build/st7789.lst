 109:Core/Src/st7789.c **** 		break;
 110:Core/Src/st7789.c **** 	case 2:
 111:Core/Src/st7789.c **** 		ST7789_WriteSmallData(ST7789_MADCTL_RGB);
 112:Core/Src/st7789.c **** 		break;
 113:Core/Src/st7789.c **** 	case 3:
 114:Core/Src/st7789.c **** 		ST7789_WriteSmallData(ST7789_MADCTL_MX | ST7789_MADCTL_MV | ST7789_MADCTL_RGB);
 115:Core/Src/st7789.c **** 		break;
 116:Core/Src/st7789.c **** 	default:
 117:Core/Src/st7789.c **** 		break;
 118:Core/Src/st7789.c **** 	}
