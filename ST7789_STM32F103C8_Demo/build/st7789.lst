  16:Core/Src/st7789.c **** void memset_16(uint16_t* buf, uint16_t value, uint32_t size) 
  17:Core/Src/st7789.c **** {
  18:Core/Src/st7789.c ****     uint16_t swapped = (value >> 8) | (value << 8); 
  19:Core/Src/st7789.c ****     while (size > 0) {
  20:Core/Src/st7789.c ****         *buf = swapped; 
  21:Core/Src/st7789.c ****         buf++;
  22:Core/Src/st7789.c ****         size--;
  23:Core/Src/st7789.c ****     }
  24:Core/Src/st7789.c **** }
